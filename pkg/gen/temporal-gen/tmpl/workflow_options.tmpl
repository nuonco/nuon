{{ if ne .OptionsCallback "" }}{{ .OptionsCallback }}(req)
{{- else -}}
workflow.ChildWorkflowOptions{
    TaskQueue:                {{ if ne .TaskQueue "" }}{{ .TaskQueue }}{{ else }}"api"{{ end }},
    WorkflowID:               {{ if ne .IDCallback "" }}{{ .IDCallback }}(req){{else}}eventloop.SignalHandlerWorkflowID(req.Signal, req.EventLoopRequest){{ end }},
    {{- if ne .ExecutionTimeout 0 }}
    WorkflowExecutionTimeout: {{ DurationLit .ExecutionTimeout }},
    {{- end }}
    {{- if ne .TaskTimeout 0 }}
    WorkflowTaskTimeout:      {{ DurationLit .TaskTimeout }},
    {{- end }}
    WorkflowIDReusePolicy:    enumsv1.WORKFLOW_ID_REUSE_POLICY_TERMINATE_IF_RUNNING,
    WaitForCancellation:      {{ if .WaitForCancellation }}true{{ else }}false{{ end }},
}{{end}}